{
  "name": "@kaweb/react-mql",
  "version": "1.0.3",
  "description": "CSS Media Queries HOC and MediaContext for React",
  "main": "lib/react-mql.js",
  "scripts": {
    "start": "NODE_ENV=production webpack --config=webpack.config.js && npm run serve",
    "serve": "node example/server.js",
    "prebuild:es6": "rm -rf lib/es && mkdir -p lib/es",
    "build:es6": "babel src/index.js -d lib/es --source-maps --minified --env-name production --no-comments",
    "prebuild:es5": "rm -rf lib",
    "build:es5": "NODE_ENV=production webpack -p --config=webpack.config.js",
    "prebuild": "npm run test",
    "build": "npm run build:es5 && npm run build:es6",
    "prepack": "rm -rf *.tgz && npm run build",
    "pack": "npm pack",
    "test": "jest --coverage",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:kumarabhishek/react-mql.git"
  },
  "keywords": [
    "react",
    "matchmedia",
    "mediaquery"
  ],
  "author": "Kumar Abhishek",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kumarabhishek/react-mql/issues"
  },
  "jest": {
    "automock": false,
    "setupTestFrameworkScriptFile": "<rootDir>/src/__test__/setup.js",
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx"
    ]
  },
  "homepage": "https://github.com/kumarabhishek/react-mql/blob/master/README.md",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "compression": "^1.7.2",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.4.1",
    "jsdom": "^11.11.0",
    "mq-polyfill": "^1.1.8",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.4.1",
    "webpack": "^4.15.0",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
